@article{ ISI:000241772800040,
Author = {Viberg, Peter and Wahlund, Karl-Gustav and Skog, Kerstin},
Title = {{On-line capillary based quantitative analysis of a heterocyclic amine in
   human urine}},
Journal = {{JOURNAL OF CHROMATOGRAPHY A}},
Year = {{2006}},
