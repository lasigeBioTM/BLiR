@article{ ISI:A1997XE48900017,
Author = {Huber, WW and McDaniel, LP and Kaderlik, KR and Teitel, CH and Lang, NP
   and Kadlubar, FF},
Title = {{Chemoprotection against the formation of colon DNA adducts from the
   food-borne carcinogen 2-amino-1-methyl-6-phenylimidazo{[}4,5-b]pyridine
   (PhIP) in the rat}},
Journal = {{MUTATION RESEARCH-FUNDAMENTAL AND MOLECULAR MECHANISMS OF MUTAGENESIS}},
Year = {{1997}},
