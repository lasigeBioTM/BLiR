@article{ ISI:000227910400007,
Author = {Frederiksen, H},
Title = {{Two food-borne heterocyclic amines: Metabolism and DNA adduct formation
   of amino-alpha-carbolines}},
Journal = {{MOLECULAR NUTRITION \& FOOD RESEARCH}},
