@article{ ISI:000221622000016,
Author = {Felton, JS and Knize, MG and Bennett, LM and Malfatti, MA and Colvin, ME
   and Kulp, KS},
Title = {{Impact of environmental exposures on the mutagenicity/carcinogenicity of
   heterocyclic amines}},
Journal = {{TOXICOLOGY}},
Year = {{2004}},
Volume = {{198}},
Number = {{1-3}},
Pages = {{135-145}},
