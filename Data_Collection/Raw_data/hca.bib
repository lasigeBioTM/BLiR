@article{ ISI:000186619300002,
Author = {Magagnotti, C and Pastorelli, R and Pozzi, S and Andreoni, B and
   Fanelli, R and Airoldi, L},
Title = {{Genetic polymorphisms and modulation of
   2-amino-1-methyl-6-phenylimidazo{[}4,5-b]pyridine (PhIP)-DNA adducts in
   human lymphocytes}},
Journal = {{INTERNATIONAL JOURNAL OF CANCER}},
