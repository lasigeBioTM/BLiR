@article{ ISI:000243799900017,
Author = {Chen, Hsiu Ling and Su, Huei Jen and Lee, Ching Chang},
Title = {{Association between tofu intake and serum polychlorinated
   dibenzo-p-dioxins and dibenzofurans (PCDD/Fs) in the elderly Taiwanese}},
Journal = {{ENVIRONMENT INTERNATIONAL}},
Year = {{2007}},
Volume = {{33}},
Number = {{2}},
Pages = {{265-271}},
Month = {{FEB}},
Abstract = {{Age and gender are related to human dietary habits, which affect serum
   levels of dioxin-like compounds via body fat content. This study
   examined whether different dietary patterns and body fat content were
   associated with serum polychlorinated dibenzo-p-dioxins and
   dibenzofirrans (PCDD/F) concentrations for different age groups of
   Taiwanese subjects. In total, 1165 volunteers completed a
   semi-quantitative food-consumption frequency questionnaire for serum
   PCDD/Fs analysis. In the elderly (age, > 45 years), a significant
   positive multivariate-adjusted association was identified between serum
   PCDD/F levels and sea fish intake. Additionally, consumption of tofu was
   negatively correlated with serum PCDD/F levels. This study also found a
   negative correlation between tofu intake and body fat content in the
   elderly. It seems that when elderly people ingested considerable amounts
   of soybean protein, such as tofu, they typically had low serum PCDD/Fs
   levels in relation to reduced body fat content and induction of
   metabolic enzymes. The relationship between tofu intake, metabolic
   enzymes, and serum PCDD/F accumulations warrants further investigated
   previously to recommend how to prevent PCDD/F accumulations via intake
   of soybean products. (c) 2006 Elsevier Ltd. All fights reserved.}},
