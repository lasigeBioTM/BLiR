@article{ ISI:000271980800111,
Author = {Lee, Ching-Chang and Shih, Tung-Seng and Chen, Hsiu-Ling},
Title = {{Distribution of air and serum PCDD/F levels of electric arc furnaces and
   secondary aluminum and copper smelters}},
Journal = {{JOURNAL OF HAZARDOUS MATERIALS}},
