@article{ ISI:A1992KC16000035,
Author = {SCHECHTER, A},
Title = {{DIOXINS AND DIBENZOFURANS IN POTENTIALLY EXPOSED WORKERS - SERIAL
   TISSUE-LEVELS IN A WORKER EXPOSED IN A PCB TRANSFORMER FIRE CLEANUP AND
   BLOOD-LEVELS IN 3 EXPOSED CHEMISTS}},
Journal = {{CHEMOSPHERE}},
Year = {{1992}},
Volume = {{25}},
Number = {{7-10}},
Pages = {{1117-1122}},
Month = {{OCT-NOV}},
Note = {{11TH INTERNATIONAL SYMP ON CHLORINATED DIOXINS AND RELATED COMPOUNDS,
   RESEARCH TRIANGLE PK, NC, SEP 23-27, 1991}},
Abstract = {{Four individuals who were potentially exposed to dioxins and
   dibenzofurans had blood and adipose tissue measurements taken which in
   some instances, documented elevated dioxin body burden as long as 35
   years after exposure. The first worker, an engineer, was involved in a
   PCB transformer fire cleanup in New York State, and had serial fat and
   blood dioxin measurements performed annually over a 7 year period. The
   other three workers were chemists. The first is an American who became
   ill after synthesis of 2,3,7,8-TBrDD and also 2,3,7,8-TCDD; his blood 35
   years after exposure, showed elevations of both congeners. The second
   chemist, also American, was involved in a chick edema disease evaluation
   from 1963-65; his blood sample taken in 1990 shows no elevation of
   dioxin and dibenzofuran congener levels. The third was a European dioxin
   chemist who had blood taken during the time he was working with various
   dioxins and dibenzofurans. Although his dioxin and dibenzofuran levels
   prior to exposure were not available, the current results are close to
   average German blood levels with possible elevation of one congener. In
   two of the four workers, blood dioxin analysis clearly demonstrated
   occupational exposure and elevated body burden.}},
