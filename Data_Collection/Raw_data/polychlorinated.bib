@article{ ISI:000165116200007,
Author = {Sweetman, AJ and Alcock, RE and Wittsiepe, J and Jones, KC},
Title = {{Human exposure to PCDD/Fs in the UK: the development of a modelling
   approach to give historical and future perspectives}},
Journal = {{ENVIRONMENT INTERNATIONAL}},
