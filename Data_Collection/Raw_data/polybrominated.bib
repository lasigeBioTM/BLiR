@article{ ISI:000270594900034,
Author = {Ahn, Ki Chang and Gee, Shirley J. and Tsai, Hsing-Ju and Bennett,
   Deborah and Nishioka, Marcia G. and Blum, Arlene and Fishman, Elana and
   Hammock, Bruce D.},
Title = {{Immunoassay for Monitoring Environmental and Human Exposure to the
   Polybrominated Diphenyl Ether BDE-47}},
Journal = {{ENVIRONMENTAL SCIENCE \& TECHNOLOGY}},
Year = {{2009}},
Volume = {{43}},
