@article{ ISI:000254485200004,
Author = {Gee, Jillian R. and Hedge, Joan M. and Moser, Virginia C.},
Title = {{Lack of alterations in thyroid hormones following exposure to
   polybrominated diphenyl ether 47 during a period of rapid brain
   development in mice}},
Journal = {{DRUG AND CHEMICAL TOXICOLOGY}},
Year = {{2008}},
Volume = {{31}},
Number = {{2}},
Pages = {{245-254}},
Abstract = {{Thyroid alterations have been shown to occur following exposure to
   polybrominated diphenyl ether (PBDE) mixtures, possibly indicating that
   disruptions in thyroid hormone levels may underlie behavior deficits
   observed in animals following postnatal PBDE exposure. This study
   determined whether acute postnatal exposure to PBDE-47 would alter
   thyroid hormones. Mice were dosed with PBDE-47 on postnatal day 10, and
   serum collected either 1, 5, or 10 days after the dose. No effect was
   observed on thyroxine and triiodothyronine levels at any age examined.
   This suggests that the neurological abnormalities reported in mice
   exposed to PBDE-47 are not due to acute changes in circulating thyroid
   hormones at these observed periods.}},
