@article{ ISI:000254776600015,
Author = {Bloom, Michael and Spliethoff, Henry and Vena, John and Shaver, Shannon
   and Addink, Rudolf and Eadon, George},
Title = {{Environmental exposure to PBDEs and thyroid function among}},
