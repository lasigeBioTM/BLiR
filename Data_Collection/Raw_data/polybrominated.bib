@article{ ISI:000262602900023,
Author = {Klosener, Johannes and Peters, Thomas M. and Adamcakova-Dodd, Andrea and
   Teesch, Lynn M. and Thorne, Peter S. and Robertson, Larry W. and Luthe,
   Gregor},
Title = {{Innovative Application of Fluoro Tagging To Trace Airborne Particulate
   and Gas-Phase Polybrominated Diphenyl Ether Exposures}},
Journal = {{CHEMICAL RESEARCH IN TOXICOLOGY}},
Year = {{2009}},
Volume = {{22}},
