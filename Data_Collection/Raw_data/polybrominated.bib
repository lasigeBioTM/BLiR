@article{ ISI:000174034700005,
Author = {Thomsen, C and Haug, LS and Leknes, H and Lundanes, E and Becher, G and
   Lindstrom, G},
Title = {{Comparing electron ionization high-resolution and electron capture
   low-resolution mass spectrometric determination of polybrominated
   diphenyl ethers in plasma, serum and milk}},
Journal = {{CHEMOSPHERE}},
Year = {{2002}},
