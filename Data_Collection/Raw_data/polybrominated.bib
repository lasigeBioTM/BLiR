@article{ ISI:000274603400002,
Author = {Chen, Chang'er and Chen, Jingwen and Zhao, Hongxia and Xie, Qing and
   Yin, Zhiqun and Ge, Linke},
Title = {{Levels and patterns of polybrominated diphenyl ethers in children's
   plasma from Dalian, China}},
Journal = {{ENVIRONMENT INTERNATIONAL}},
Year = {{2010}},
Volume = {{36}},
Number = {{2}},
Pages = {{163-167}},
Month = {{FEB}},
Abstract = {{Polybrominated diphenyl ethers (PBDEs) are potential persistent organic
   pollutants which have raised many concerns in recent years. Research
   focusing on children exposure to PBDEs is important but insufficient.
   The levels and patterns of PBDEs in children's plasma from Dalian, China
   were studied for the first time. Seventeen PBDE congeners (BDE-30, 28,
   35, 37, 75, 47, 66,100, 99,116, 155,154,153, 183, 181, 190 and 209) in
   29 plasma samples were measured. Median PBDE concentration was 31.61 ng
   g(-1) lipid. BDE-153 was the dominant congener, followed by BDE-99, 47,
