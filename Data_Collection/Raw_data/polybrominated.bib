@article{ ISI:000260086800009,
Author = {Anderson, Henry A. and Imm, Pamela and Knobeloch, Lynda and Turyk, Mary
   and Mathew, John and Buelow, Carol and Persky, Victoria},
Title = {{Polybrominated diphenyl ethers (PBDE) in serum: Findings from a US
   cohort of consumers of sport-caught fish}},
Journal = {{CHEMOSPHERE}},
