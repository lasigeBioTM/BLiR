Author = {Hurley, Susan and Reynolds, Peggy and Goldberg, Debbie and Nelson, David
   O. and Jeffrey, Stefanie S. and Petreas, Myrto},
Title = {{Adipose levels of polybrominated diphenyl ethers and risk of breast
   cancer}},
Journal = {{BREAST CANCER RESEARCH AND TREATMENT}},
Year = {{2011}},
Volume = {{129}},
