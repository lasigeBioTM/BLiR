@article{ ISI:000287620100007,
Author = {Kalantzi, Olga I. and Geens, Tinne and Covaci, Adrian and Siskos,
   Panayiotis A.},
Title = {{Distribution of polybrominated diphenyl ethers (PBDEs) and other
   persistent organic pollutants in human serum from Greece}},
Journal = {{ENVIRONMENT INTERNATIONAL}},
