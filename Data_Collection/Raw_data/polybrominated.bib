@article{ ISI:000261936700039,
Author = {Keller, Jennifer M. and Swarthout, Robert F. and Carlson, Brianna K. R.
   and Yordy, Jennifer and Guichard, Aurore and Schantz, Michele M. and
   Kucklick, John R.},
Title = {{Comparison of five extraction methods for measuring PCBs, PBDEs,
   organochlorine pesticides, and lipid content in serum}},
Journal = {{ANALYTICAL AND BIOANALYTICAL CHEMISTRY}},
Year = {{2009}},
Volume = {{393}},
