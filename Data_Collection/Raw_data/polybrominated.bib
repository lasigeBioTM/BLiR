Author = {Darnerud, Per Ola and Aune, Marie and Larsson, Lotta and Hallgren, Sara},
Title = {{Plasma PBDE and thyroxine levels in rats exposed to Bromkal or BDE-47}},
Journal = {{CHEMOSPHERE}},
Year = {{2007}},
Volume = {{67}},
Number = {{9, SI}},
Pages = {{S386-S392}},
Month = {{APR}},
Note = {{24th International Symposium on Halogenated Environmental Organic
