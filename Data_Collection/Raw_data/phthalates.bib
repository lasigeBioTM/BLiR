@article{ ISI:000304337300025,
Author = {Olsen, Lena and Lind, Lars and Lind, P. Monica},
Title = {{Associations between circulating levels of bisphenol A and phthalate
   metabolites and coronary risk in the elderly}},
Journal = {{ECOTOXICOLOGY AND ENVIRONMENTAL SAFETY}},
Year = {{2012}},
Volume = {{80}},
Pages = {{179-183}},
Month = {{JUN 1}},
Abstract = {{Studies have pointed out associations between various chemicals with
   estrogenic activity and cardiovascular disease. Being ubiquitous, the
   plastic additive substances bisphenol A (BPA), and phthalates have been
   detected in almost all types of analyzed human samples. The aim of this
   study was to investigate whether circulating levels of BPA and/or four
   selected phthalate metabolites are associated to coronary risk in an
   elderly population. In the Prospective Investigation of the Vasculature
   in Uppsala Seniors (PIVUS) study, coronary risk was assessed by the
   Framingham Risk Score (FRS) together with circulating serum levels of
   BPA and the four phthalate metabolites monoisobutyl phthalate (MiBP),
   monomethyl phthalate (MMP), monoethyl phthalate (MEP),
   mono-(2-ethylhexyl) phthalate (MEHP) in 1016 subjects aged 70 years.
   BPA, MEHP, and MMP were associated to LDL-cholesterol and MEHP to
   HDL-cholesterol, MEP to diastolic blood pressure and MiBP to fasting
   glucose when the compounds were investigated one by one. After
