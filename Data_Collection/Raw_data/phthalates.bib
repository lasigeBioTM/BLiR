@article{ ISI:A1997WK62200015,
Author = {Nagini, S and Selvam, S},
Title = {{Biochemical indicators of membrane damage in the plasma and erythrocytes
   of rats fed the peroxisome proliferator di(2-ethylhexyl)phthalate}},
Journal = {{MEDICAL SCIENCE RESEARCH}},
Year = {{1997}},
Volume = {{25}},
Number = {{2}},
Pages = {{119-121}},
Month = {{FEB}},
Abstract = {{We examined the deleterious effects induced by the plasticiser
   di(2-ethylhexyl)phthalate (DEHP) in the plasma and erythrocytes of rats.
   Administration of DEHP for 24 weeks caused decreases in erythrocyte
   count and uric acid concentration and an increase in the levels of
   methaemoglobin. The increase in erythrocyte osmotic fragility, which is
   inversely related to erythrocyte survival, correlated well with a
   decrease in red cell Na+K+-ATPase activity. The release of the lysosomal
   enzyme acid phosphatase and the metal catalysts iron and calcium can
   account for the liver cell damage induced by DEHP.}},
