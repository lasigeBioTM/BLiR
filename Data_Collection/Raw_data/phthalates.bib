@article{ ISI:000285325200016,
Author = {Xu, Chuan and Chen, Ji-An and Qiu, Zhiqun and Zhao, Qing and Luo,
   Jiaohua and Yang, Lan and Zeng, Hui and Huang, Yujing and Zhang, Liang
   and Cao, Jia and Shu, Weiqun},
Title = {{Ovotoxicity and PPAR-mediated aromatase downregulation in female
   Sprague-Dawley rats following combined oral exposure to benzo{[}a]pyrene
   and di-(2-ethylhexyl) phthalate}},
Journal = {{TOXICOLOGY LETTERS}},
Year = {{2010}},
Volume = {{199}},
Number = {{3}},
Pages = {{323-332}},
Month = {{DEC 15}},
Abstract = {{The aim of the present study was to determine the ovotoxicity of female
   Sprague-Dawley (SD) rats exposed to benzo{[}a]pyrene (B{[}a]P) and
   di-(2-ethylhexyl) phthalate (DEHP), either alone or in combination; the
   molecular mechanism and the combined effects were also evaluated. Female
   rats were given intragastric administration of control (corn oil),
   B{[}a]P (5 and 10 mg/kg), DEHP (300 and 600 mg/kg) and B{[}a]P + DEHP
   (at 5 mg/kg and 300 mg/kg respectively, or at 10 mg/kg and 600 mg/kg
   respectively) on alternate days for 60 days. Relative ovary weight,
   estrous cycle, 17 beta-estradiol blood level, ovarian follicle
   populations, granulosa cell apoptosis, and gene and protein expression
   of P450Arom and PPAR were investigated. Our study demonstrated that the
   combination of B{[}a]P and DEHP exerts ovotoxicity in female rats and
   suppression of sex hormone secretion and homeostasis, which is
   associated with prolonged estrous cycles, decreases in ovarian follicle
   populations and granulosa cell apoptosis involving a PPAR-mediated
   signaling pathway of action of the two chemicals. In addition, based on
   qualitative assessment of the combined toxicity, no interaction effects
   were observed following combined B{[}a]P and DEHP administration. (C)
   2010 Elsevier Ireland Ltd. All rights reserved.}},
