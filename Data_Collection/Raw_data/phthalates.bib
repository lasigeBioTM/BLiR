@article{ ISI:000229136100016,
Author = {Nuti, F and Hildenbrand, S and Chelli, M and Wodarz, R and Papini, AM},
Title = {{Synthesis of DEHP metabolites as biomarkers for GC-MS evaluation of
   phthalates as endocrine disrupters}},
Journal = {{BIOORGANIC \& MEDICINAL CHEMISTRY}},
Year = {{2005}},
Volume = {{13}},
Number = {{10}},
Pages = {{3461-3465}},
Month = {{MAY 16}},
Abstract = {{Phthalates are used primarily as plasticizers to make polyvinyl chloride
   (PVC) soft and flexible. In recent years the phthalate esters have
   attracted increasing attention as environmental and biomedical
   pollutants and, because of their toxicological characteristics. In
   particular, they are more and more recognized as endocrine disrupters.
   In this context, we describe herein an efficient synthetic pathway
   leading to a series of metabolites of di(2-ethylhexyl) phthalate (DEHP).
   Mono(2-ethylhexenyl) phthalate was used as starting material to obtain
   these products in good yield, large scale and GC-MS purity. The
   metabolites of DEHP were synthesized, for the first time, as biomarkers
   to verify their quantitative determination in human urine and serum by
   GC-MS analysis for studying the exposure to phthalates and establishing
   reference values. \&COPY; 2005 Elsevier Ltd. All rights reserved.}},
