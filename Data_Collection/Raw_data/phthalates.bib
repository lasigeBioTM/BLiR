@article{ ISI:000253710200010,
Author = {Janjua, Nadeem Rezaq and Frederiksen, Hanne and Skakkebaek, Niels E. and
   Wulf, Hans Christian and Andersson, Anna-Maria},
Title = {{Urinary excretion of phthalates and paraben after repeated whole-body
   topical application in humans}},
