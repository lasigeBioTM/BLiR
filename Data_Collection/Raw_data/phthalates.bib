@article{ ISI:000244592600004,
Author = {Wittassek, Matthias and Heger, Wolfgang and Koch, Holger M. and Becker,
   Kerstin and Angerer, Juergen and Kolossa-Gehring, Marike},
Title = {{Daily intake of di(2-ethylhexyl)phthalate (DEHP) by German children - A
   comparison of two estimation models based on urinary DEHP metabolite
   levels}},
Journal = {{INTERNATIONAL JOURNAL OF HYGIENE AND ENVIRONMENTAL HEALTH}},
Year = {{2007}},
Volume = {{210}},
Number = {{1}},
Pages = {{35-42}},
Month = {{JAN}},
Abstract = {{Di(2-ethylhexyl)phthalate (DEHP) is a general-purpose plasticizer for
   polyvinyl chloride (PVC) and has become a ubiquitous environmental
   contaminant. It is suspected to be an endocrine disruptin/modulating
   substance in humans. Children are of special concern due to their
   developmental state. In our study we estimated the daily DEHP intake of
   239 children aged 2-14 years by extrapolating from their urinary levels
   of the DEHP metabolites mono-(2-ethyl-5-hyd roxyhexyl)phthalate
   (50H-MEHP), mono-(2-ethyl-5-oxohexyl)phthalate (5oxo-MEHP) and
   mono-(2-ethylhexyl)phthalate (MEHP). We applied two calculation models
   based upon the volume and the creatinine-related urinary metabolite
   concentrations. Applying the volume- or the creatinine-based calculation
   model we determined a median daily DEHP intake of 7.8 or 4.3 mu g/kg
   body weight (bw)/day and a 95th percentile of 25.2 or 15.2 mu g/kg
   bw/day. Three children (1\%) exceeded the value of the tolerable daily
   intake (TDI) of the European Food Safety Authority of 50 mu g/ kg
   bw/day, while 7.5\% or 3\% (depending on the calculation model) exceeded
   the reference dose (RfD) of 20 mu g/kg bw/day of the US Environmental
   Protection Agency. In general, DEHP exposure was decreasing with
   increasing age and boys had higher exposures than girls. Our findings
   suggest that the majority of the children in the general population is
   exposed to quantities of DEHP below the TDI and the RfD. However, many
   children scoop out the preventive limit values to a considerable degree
   and in individual cases we observed substantial transgressions. Younger
   children seem to be more severely burdened, which may be due to a higher
   food consumption related to their bw, mouthing behaviour and/or playing
   near the ground. (c) 2006 Elsevier GmbH. All rights reserved.}},
Publisher = {{ELSEVIER GMBH, URBAN \& FISCHER VERLAG}},
